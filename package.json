    "docs:redirects": "cp ./_redirects ./docs/.vuepress/dist/",
    "serve-static": "./node_modules/.bin/serve -l 3030 ./docs/.vuepress/dist/",
    "textlint": "./node_modules/.bin/textlint 'docs/**'",
